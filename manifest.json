      "<all_urls>"
    ],
    "host_permissions": [
      "https://*.twitch.tv/*"
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "short_name": "MTV",
    "version": "0.0.1"
  }