{
    "action": {
      "default_icon": {

      },
      "default_popup": "popup.html"
    },
    "author": "Zach Gannon",
    "background": {
      "service_worker": "/js/background.js",
      "type": "module"
    },
    "description": "When you want to get all the perspectives at once.",
    "icons": {
    },
    "manifest_version": 3,
